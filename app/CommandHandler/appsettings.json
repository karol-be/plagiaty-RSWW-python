{
  "ConnectionStrings": {
    "EventStore": "esdb://eventstore:2113?tls=false"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "rabbitmq": {
    "Username": "admin",
    "Password": "admin",
    "VirtualHost": "/",
    "Port": 5672,
    "Hostnames": [
      "rabbitmq"
    ],
    "RequestTimeout": "00:00:10",
    "PublishConfirmTimeout": "00:00:01",
    "RecoveryInterval": "00:00:10",
    "PersistentDeliveryMode": true,
    "AutoCloseConnection": true,
    "AutomaticRecovery": true,
    "TopologyRecovery": true,
    "Exchange": {
      "Durable": true,
      "AutoDelete": true,
      "Type": "Topic"
    },
    "Queue": {
      "AutoDelete": true,
      "Durable": true,
      "Exclusive": true
    }
  },
  "jaeger": {
    "enabled": true,
    "serviceName": "CommandHandler",
    "udpHost": "jaeger",
    "udpPort": 6831,
    "maxPacketSize": 65000,
    "sampler": "const",
    "maxTracesPerSecond": 10,
    "excludePaths": ["/", "/ping", "/metrics"]
  },
  "scripts": {
    "performAnalysis": "./spark_analysis_scripts/spark_plagiatarism_detection.py",
    "uploadSource": "./spark_analysis_scripts/spark_save_source_file.py",
    "uploadAnalysis": "./spark_analysis_scripts/spark_save_file_to_analysis.py"
  }
}
